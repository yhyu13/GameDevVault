          Drupal.nvidiaOgMembership.check($('#edit-mail'));
          Drupal.nvidiaOgMembership.buttonStatus();
          if((!firstValid) || (!lastValid) || (!organizationValid) || (!countryValid) || (!industriesValid) || (!developmentAreasValid) || (!yearsOld) ||  (!acceptTherms) || (!Drupal.nvidiaOgMembership.validEmail)) {
            return false;
          }
        });

        Drupal.nvidiaOgMembership.buttonStatus();
      }

      // Validation of Steps.
      function stepsValidation(formID, submitID) {
        if ($(formID).length) {

          var validGroup = [],
              validText = [],
              validSelect = [],
              notValid;

          var message = '<span class="error-message"><i class="fa fa-times"></i>Cannot be left blank</span><span class="valid-message"><i class="fa fa-check"></i>Looks good!</span>';
          var message2 = '<span class="error-message"><i class="fa fa-times"></i>Not correct</span>';

          // DZWEB-173: Number Fields have incorrect type and incorrect data validation
          var message_numeric_field = '<span class="error-message"><i class="fa fa-times"></i>Value must be an integer and cannot be blank</span><span class="valid-message"><i class="fa fa-check"></i>Looks good!</span>';
          // DZWEB-219: Number field validation for non-required fields
          var message_non_req_numeric_field = '<span class="error-message"><i class="fa fa-times"></i>Value must be an integer</span>';

          // Disalow Enter button on form element except textarea.
          $(formID).on('keyup keypress', function(e) {
            var keyCode = e.keyCode || e.which;
            if (keyCode === 13 && e.target.type !== 'textarea') {
              e.preventDefault();
              return false;
            }
          });

          function showMessages(element, variable) {
            // DZWEB-62: Broken validation, broken messages placing
            // Some specific if Location fieldset is in use
            if (element.parents("fieldset.location").length) {
              if(variable) {
                element.parent().find('.valid-message').show();
                element.parent().find('.error-message').hide();
              }else {
                element.parent().find('.valid-message').hide();
                element.parent().find('.error-message').show();
              }
            }
            else {
              if(variable) {
                element.parent().parent().find('.valid-message').show();
                element.parent().parent().find('.error-message').hide();
              }else {
                element.parent().parent().find('.valid-message').hide();
                element.parent().parent().find('.error-message').show();
              }
            }
          }

          // DZWEB-206: Inception Program - GPU step logic.
          function gpuStepLogic(element) {
            // Field: Do you use GPUs today, either in the cloud or on-premise? * - start point
            var pairedElements = ['edit-field-gpu-prem-csp-und-in-the-cloud', 'edit-field-gpu-prem-csp-und-on-premise'];
            var listElements = $('div#edit-field-gpu-prem-csp').nextAll();
            // If element was checked.
            if (element.is(":checked")) {
              // Check if on of logically paired variants from start point is checked.
              if (element.parents('div#edit-field-gpu-prem-csp').length && pairedElements.includes(element.attr('id'))) {
                // Make negative variant's field clean and disabled.
                $('div#edit-field-non-gpus-use').find('input').prop('checked', '').trigger("change");
                element.parents('div#edit-field-gpu-prem-csp').find('input[id="edit-field-gpu-prem-csp-und-not-using-gpus"]').prop({disabled: 'disabled', checked: ''}).trigger("change");
              }
            }
            // If element was unchecked.
            else {
              // Check if one of of logically paired variants from start point is still checked.
              var currentpairedElements = element.parents('div#edit-field-gpu-prem-csp').find('input:checked:not([id="edit-field-gpu-prem-csp-und-not-using-gpus"])');
              // If not - make negative variant enabled. Perform cleaning job.
              if (pairedElements.includes(element.attr('id')) && currentpairedElements.length === 0) {
                element.parents('div#edit-field-gpu-prem-csp').find('input[id="edit-field-gpu-prem-csp-und-not-using-gpus"]').prop('disabled', '').trigger("change");
                $('div#edit-field-gpu-use-now-other').find('input:text').val('').trigger("change");
                listElements.each(function(index) {
                  $(this).find('input:checked').prop('checked', '').trigger("change");
                });
              }
              // If so - clean only descendent fields.
              else if (pairedElements.includes(element.attr('id'))) {
                switch(element.attr('id')) {
                  case 'edit-field-gpu-prem-csp-und-on-premise':
                      $('div#edit-field-on-prem-gpus').find('input:checked').prop('checked', '').trigger("change");
                    break;
                  case 'edit-field-gpu-prem-csp-und-in-the-cloud':
                      $('div#edit-field-csp-gpus').find('input:checked').prop('checked', '').trigger("change");
                    break;
                }
              }
              // If negative variant is checked - clean descendent field.
              else if (element.parents('div#edit-field-gpu-prem-csp').length && element.attr('id') === 'edit-field-gpu-prem-csp-und-not-using-gpus') {
                $('div#edit-field-non-gpus-use').find('input').prop('checked', '').trigger("change");
              }
              // If "other" variant is checked - clean descendent field.
              else if (element.attr('id') === 'edit-field-gpu-use-now-und-other-please-specify-below') {
                $('div#edit-field-gpu-use-now-other').find('input:text').val('').trigger("change");
              }
            }

            validGroup = [];
            checkboxAreaStatus();
            isFormValid();
            nextButtonStatus();
          }

          // DZWEB-219: Number field validation for non-required fields
          function isIntNumber(n) {
            if (!isNaN(parseFloat(n)) && isFinite(n) && (n % 1 === 0)) {
              return true;
            }
            else return false;
          }

          function textInputsVerification(element) {
            var value = element.val().trim();
            // For url
            if(element.parent().parent().hasClass('field-value-url')){
              var pattern = new RegExp('^(https?:\\/\\/)?'+ // protocol
              '((([a-z\\d]([a-z\\d-]*[a-z\\d])*)\\.)+[a-z]{2,}|'+ // domain name
              '((\\d{1,3}\\.){3}\\d{1,3}))'+ // OR ip (v4) address
              '(\\:\\d+)?(\\/[-a-z\\d%_.~+]*)*'+ // port and path
              '(\\?[;&a-z\\d%_.~+=-]*)?'+ // query string
              '(\\#[-a-z\\d_]*)?$','i'); // fragment locator;
              if (pattern.test(value)) {
                return true;
              }
              else {
                if(!pattern.test(value) && value !== '')
                  element.parent().parent().find('.error-message').html('<i class="fa fa-times"></i>Not correct');
                if(value === '')
                  element.parent().parent().find('.error-message').html('<i class="fa fa-times"></i>Cannot be left blank');
                return false;
              }
            }
            // DZWEB-173: Number Fields have incorrect type and incorrect data validation
            else if(element.hasClass('number-integer')) {
              if (element.hasClass('required') && (value === '' || !isIntNumber(value))) {
                return false;
              }
              // DZWEB-219: Number field validation for non-required fields
              if (!element.hasClass('required') && value !== '' && !isIntNumber(value)) {
                return false;
              }
              else {
                return true;
              }
            } else {
              if(value === '')
                return false;
              else
                return true;
            }
          }

          // DZWEB-98
          // Validate select if option wasnt selected.
          function selectVerification(element) {
            if(element.val() == '_none' || element.val() == '')
              return false;
            else
              return true;
          }

          function checkboxAreaStatus(simple) {
            $(''+formID+' .form-checkboxes, '+formID+' .form-radios').each(function(index) {
              if($(this).parent().find('span.form-required').length) {
                validGroup[index] = false;
                // DZWEB-102: Make cardinality validation on Non-required checkboxes.
                var cardinality = $(this).attr('class').match('ogm-cardinality-([0-9]*)');
                $(this).find('input').each(function() {
                  if(!($(this).prop('checked')) && $(this).val() == 'select_or_other'){
                    $(this).parents('.form-type-checkboxes, .form-type-radios').find('.error-message').html('<i class="fa fa-times"></i>Cannot be left blank');
                  }
                  if($(this).prop('checked') && $(this).val() != 'select_or_other'){
                    validGroup[index] = true;
                  }
                  if($(this).prop('checked') && $(this).val() == 'select_or_other'){
                    var otherInput = $(this).parents('.select-or-other').find('.select-or-other-other'),
                        otherValue = otherInput.val();
                    if((otherValue) == '') {
                      $(this).parents('.form-type-checkboxes, .form-type-radios').find('.error-message').html('<i class="fa fa-times"></i>Please fill out this field');
                      validGroup[index] = false;
                    }else {
                      validGroup[index] = true;
                    }
                  }
                  if (cardinality) {
                    var cardinality_number = Number(cardinality[1]);
                    // Check if cardinality number is the same to checked inputs amount.
                    // Check if cardinality number > then checked.
                    if (cardinality_number == $(this).parents('.form-checkboxes').find('input:checked').length){
                      $(this).parents('.form-checkboxes').find('input:not(:checked)').prop('disabled', true);
                    }
                    else if (cardinality_number > $(this).parents('.form-checkboxes').find('input:checked').length){
                      $(this).parents('.form-checkboxes').find('input:disabled').prop('disabled', false);
                    }
                  }

                  if(!simple)
                    showMessages($(this).parents('.form-checkboxes, .form-radios'), validGroup[index]);
                });
              }

              // Other field in not required areas
              if(!($(this).parent().find('span.form-required').length)&&($(this).parent().find('input[value="select_or_other"]').length)){
                var element = $(this).parent().find('input[value="select_or_other"]');
                var otherInput = $(this).parents('.select-or-other').find('.select-or-other-other');
                var otherValue = otherInput.val();
                $(this).find('input').each(function(){
                  $(this).on('change', function(){
                    if(element.prop('checked')){
                        otherValue = otherInput.val();
                        if((otherValue) == '') {
                          validGroup[index] = false;
                        }else {
                          validGroup[index] = true;
                        }
                        if(!simple)
                          showMessages(element.parent(), validGroup[index]);
                        isFormValid();
                        nextButtonStatus();
                    }else {
                      validGroup[index] = true;
                    }
                    if(!simple)
                      showMessages(element.parent(), validGroup[index]);
                    isFormValid();
                    nextButtonStatus();
                  });
                });
              }
            });
          }

          function textInputsStatus(simple) {
            $(''+formID+' input[type="text"], '+formID+' textarea').each(function(index) {
              if($(this).hasClass('required') ||
                $(this).parents('.field-widget-date-popup').find('span.form-required').length ||
                $(this).hasClass('number-integer')) {
                 validText[index] = textInputsVerification($(this));
                if(!simple)
                  showMessages($(this), validText[index]);
              }
            });
          }

          function selectBoxStatus(simple) {
            $(''+formID+' select').each(function(index) {
              if($(this).hasClass('required')) {
                validSelect[index] = selectVerification($(this));
                if(!simple)
                  showMessages($(this), validSelect[index]);
              }
            });
          }

          function nextButtonStatus() {
            if(notValid)
              $(submitID).addClass('notValid');
            else
              $(submitID).removeClass('notValid');
          }

          function checkArray(array) {
            var valid = true;
            for(var i=0; i < array.length; i++) {
              if(array[i] == false)
                valid = false;
            }
            if(!valid)
              return false;
            else
              return true;
          }

          function isFormValid() {
            var validGroups = checkArray(validGroup),
                validTextInputs = checkArray(validText),
                validSelects = checkArray(validSelect);
            if((!validGroups) || (!validTextInputs) || (!validSelects))
              notValid = true;
            else
              notValid = false;
          }

          function requiredTextfieldsVerification() {
            $('' + formID + ' input[type="text"], ' + formID + ' textarea').each(function (index) {
              if ($(this).hasClass('required') ||
                $(this).parents('.field-widget-date-popup').find('span.form-required').length ||
                $(this).hasClass('number-integer')) {
                // DZWEB-62: Broken validation, broken messages placing
                // Some specific if Location fieldset is in use
                if ($(this).parents('fieldset.location').length) {
                  if ($(this).parent().find('.error-message').length <= 0) {
                    $(this).parent().append(message);
                  }
                }
                // DZWEB-173: Number Fields have incorrect type and incorrect data validation
                else if ($(this).hasClass('number-integer') && $(this).hasClass('required')) {
                  if ($(this).parent().parent().find('.error-message').length <= 0) {
                    $(this).parent().parent().append(message_numeric_field);
                  }
                }
                // DZWEB-219: Number field validation for non-required fields
                else if ($(this).hasClass('number-integer')) {
                  if ($(this).parent().parent().find('.error-message').length <= 0) {
                    $(this).parent().parent().append(message_non_req_numeric_field);
                  }
                }
                 else {
                  if ($(this).parent().parent().find('.error-message').length <= 0) {
                    $(this).parent().parent().append(message);
                  }
                }

                $(this).on('blur', function () {
                  validText[index] = textInputsVerification($(this));
                  showMessages($(this), validText[index]);
                  isFormValid();
                  nextButtonStatus();
                });
              }
            });
          }

          function checkboxGroupValidation(checkboxes) {
            var valid = false;

            if (checkboxes.length <= 0) {
              return false;
            }

            checkboxes.each(function () {
              if ($(this).attr('checked')) {
                valid = true;
              }
            });

            return valid;
          }

          // Verification of checkboxes and radio buttons in groups
          $(''+formID+' .form-checkboxes, '+formID+' .form-radios').each(function(index) {
            // DZWEB-102: Make cardinality validation on checkboxes.
            var cardinality = $(this).attr('class').match('ogm-cardinality-([0-9]*)');
            // DZWEB-206: Inception Program - GPU step fields list.
            // All of them considered to be required (at the moment or later in logic of Conditional Fields)
            var gpuStep = ['edit-field-gpu-prem-csp', 'edit-field-using-gpus',
              'edit-field-non-gpus-use', 'edit-field-gpu-use-now',
              'edit-field-on-prem-gpus', 'edit-field-csp-gpus'];
            if($(this).parent().find('span.form-required').length ||
              gpuStep.includes($(this).parent().parent().attr('id'))) {
              // DZWEB-206: Inception Program - GPU step logic.
              if ($(this).parent().parent().attr('id') === 'edit-field-gpu-prem-csp') {
                if ($(this).parent().parent().find('input[type="checkbox"]:checked:not([id="edit-field-gpu-prem-csp-und-not-using-gpus"])').length) {
                  $(this).parent().parent().find('input[id="edit-field-gpu-prem-csp-und-not-using-gpus"]').prop('disabled', 'disabled');
                }
              }
              $(this).parent().append(message);
              $(this).find('input').each(function(){
                if($(this).attr('type') == 'checkbox')
                  var element = $(this).parent().parent().find('input[type="checkbox"]');
                if($(this).attr('type') == 'radio')
                  var element = $(this).parent().parent().find('input[type="radio"]');
                $(this).on('change', function() {
                  // DZWEB-206: Inception Program - GPU step logic.
                  if (gpuStep.includes($(this).parents('.field-type-list-text').attr('id'))) {
                    gpuStepLogic(element);
                  }
                  validGroup[index] = false;
                  element.each(function(){
                    if(!($(this).prop('checked')) && $(this).val() == 'select_or_other'){
                      $(this).parents('.form-type-checkboxes, .form-type-radios').find('.error-message').html('<i class="fa fa-times"></i>Cannot be left blank');
                    }
                    if($(this).prop('checked') && $(this).val() != 'select_or_other'){
                      validGroup[index] = true;
                    } else {
                      var checkboxesGroup = $(this).parents('.form-checkboxes').find('input');
                      validGroup[index] = checkboxGroupValidation(checkboxesGroup);
                    }

                    if($(this).prop('checked') && $(this).val() == 'select_or_other'){
                      var otherInput = $(this).parents('.select-or-other').find('.select-or-other-other'),
                          otherValue = otherInput.val();
                      otherInput.on('blur',function(){
                        otherValue = otherInput.val();
                        if(otherValue == ''){
                          validGroup[index] = false;
                          $(this).parents('.select-or-other').find('.valid-message').hide();
                          $(this).parents('.select-or-other').find('.error-message').show();
                        }else{
                          validGroup[index] = true;
                          $(this).parents('.select-or-other').find('.valid-message').show();
                          $(this).parents('.select-or-other').find('.error-message').hide();
                        }
                        isFormValid();
                        nextButtonStatus();
                      });
                      if((otherValue) == '') {
                        $(this).parents('.form-type-checkboxes, .form-type-radios').find('.error-message').html('<i class="fa fa-times"></i>Please fill out this field');
                        validGroup[index] = false;
                      }else {
                        validGroup[index] = true;
                      }
                    }
                    if (cardinality) {
                      var cardinality_number = Number(cardinality[1]);
                      // Check if cardinality number is the same to checked inputs amount.
                      // Check if cardinality number > then checked.
                      if (cardinality_number == $(this).parents('.form-checkboxes').find('input:checked').length){
                        $(this).parents('.form-checkboxes').find('input:not(:checked)').prop('disabled', true);
                      }
                      else if (cardinality_number > $(this).parents('.form-checkboxes').find('input:checked').length){
                        $(this).parents('.form-checkboxes').find('input:disabled').prop('disabled', false);
                      }
                    }
                    showMessages(element.parents('.form-checkboxes, .form-radios'), validGroup[index]);
                  });
                  isFormValid();
                  nextButtonStatus();
                });
              });
            }

            if(!($(this).parent().find('span.form-required').length) && cardinality){
              $(this).on('change', function() {
                validGroup[index] = true;
                if (cardinality) {
                  var cardinality_number = Number(cardinality[1]);
                  // Check if cardinality number is not the same to inputs amount.
                  // Check if cardinality number less then checked.
                  if (cardinality_number !== $(this).find('input').length
                    && cardinality_number < $(this).find('input:checked').length){
                      validGroup[index] = false;
                  }
                }
                isFormValid();
                nextButtonStatus();
              });
            }

            // Other field in not required areas
            if(!($(this).parent().find('span.form-required').length)&&($(this).parent().find('input[value="select_or_other"]').length)){
              $(this).parent().append('<span class="error-message"><i class="fa fa-times"></i>Please fill out this field</span>');
              var element = $(this).parent().find('input[value="select_or_other"]');
              var otherInput = $(this).parents('.select-or-other').find('.select-or-other-other');
              var otherValue = otherInput.val();
              $(this).find('input').each(function(){
                $(this).on('change', function(){
                  if(element.prop('checked')){
                    otherInput.on('blur',function(){
                      otherValue = otherInput.val();
                      if((otherValue) == '') {
                        validGroup[index] = false;
                      }else {
                        validGroup[index] = true;
                      }
                      showMessages(element.parent(), validGroup[index]);
                      isFormValid();
                      nextButtonStatus();
                    });
                  }else {
                    validGroup[index] = true;
                  }
                  showMessages(element.parent(), validGroup[index]);
                  isFormValid();
                  nextButtonStatus();
                });
              });
            }
          });

          // DZWEB-104
          // Validate required fields after form state changed.
          $(''+formID+' input[type="text"], '+formID+' textarea').each(function (index) {
            $(this).attrchange({
              trackValues: true,
              callback: function (event) {
                if(event.attributeName == "class") {
                  if (event.newValue.includes('required')) {
                    if ($(this).parent().parent().find('.error-message').length <= 0) {
                      $(this).parent().parent().append(message);
                    }
                    validText[index] = textInputsVerification($(this));
                    $(this).on('blur', function() {
                      validText[index] = textInputsVerification($(this));
                      showMessages($(this), validText[index]);
                      isFormValid();
                      nextButtonStatus();
                    });
                  }
                  else if ($(this).parents('.field-widget-date-popup').find('span.form-required').length) {
                    $(this).on('change', function() {
                      validText[index] = textInputsVerification($(this));
                      showMessages($(this), validText[index]);
                      isFormValid();
                      nextButtonStatus();
                    });
                  }
                  else {
                    validText[index] = true;
                  }
                  showMessages($(this), validText[index]);
                  isFormValid();
                  nextButtonStatus();
                }
              }
            });
          });

          // Required text inputs verification
          requiredTextfieldsVerification();

          // Required selects verification
          $(''+formID+' select').each(function(index) {
            if($(this).hasClass('required')) {
              $(this).parent().append(message);
              $(this)
              .on('change', function() {
                validSelect[index] = selectVerification($(this));
                showMessages($(this), validSelect[index]);
                isFormValid();
                nextButtonStatus();
              })
              .on('blur', function(){
                validSelect[index] = selectVerification($(this));
                showMessages($(this), validSelect[index]);
                isFormValid();
                nextButtonStatus();
              });
            }
          });

          $(''+formID+' .error-message, '+formID+' .valid-message').hide();

          $(submitID).on('click', function() {
            textInputsStatus();
            selectBoxStatus();
            checkboxAreaStatus();
            isFormValid();
            nextButtonStatus();
            if(notValid)
              return false;
          });

          textInputsStatus(true);
          checkboxAreaStatus(true);
          selectBoxStatus(true);
          isFormValid();
          nextButtonStatus();
        }
      }

      stepsValidation('form[id^="nvidia-og-membership-registration-form"].nvidia-og-membership-registration-form-membership-step', '#edit-next, #edit-finish, #edit-draft');
      stepsValidation('form[id^="og-ui-edit-membership"]', '#edit-submit, #edit-send');
    }
  };

