            if(element.hasClass('required') || element.parent().parent().hasClass('link-field-url') || element.parent().hasClass('form-item-mail')){
              trueCondition = true;
            }
            if(trueCondition) {
              element.parent().append(message);
              element.on('blur', function(){
                textInputs[index] = {element: element};
